{
    "manifest_version": 3,
    "name": "Auto Class Registration",
    "version": "1.0",
    "description": "Automatically register for classes on the Banner system.",
    "permissions": ["activeTab", "scripting"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "https://banner.apps.uillinois.edu/*"
    ]
}
